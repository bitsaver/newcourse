<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.yang.newcourse.mapper.StudentAnswerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="pers.yang.newcourse.entity.StudentAnswer">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="quiz_id" property="quizId" />
        <result column="question_id" property="questionId" />
        <result column="answer" property="answer" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, quiz_id, question_id, answer
    </sql>

</mapper>
